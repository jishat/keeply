{
    "manifest_version": 3,
    "name": "Keeply",
    "version": "1.0.0",
    "description": "A Chrome extension built with React and Vite",
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Keeply"
    },
    "background": {
      "service_worker": "assets/background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["assets/content.js"],
        "run_at": "document_end"
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["assets/*"],
        "matches": ["<all_urls>"]
      }
    ],
    "chrome_url_overrides": {
        "newtab": "popup.html"
    }
  }